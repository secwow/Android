<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dima.rebenko.notebook">
    <permission android:name="dima.rebenko.notebook.READ_DATABASE" android:protectionLevel="normal" />
    <permission android:name="dima.rebenko.notebook.WRITE_DATABASE" android:protectionLevel="normal" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.GreenTheme">
        <activity
            android:name=".Navigation"
            android:label="@string/title_activity_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.EditAddActivity"/>
        <activity android:name=".activity.MainActivity"/>
        <activity android:name=".activity.RGB"/>
        <activity android:name=".activity.CalclulatorFragment" />
        <provider android:name=".Helpers.RealmDB"
            android:authorities="dima.rebenko.notebook"
            android:readPermission="dima.rebenko.notebook.READ_DATABASE"
            android:writePermission="dima.rebenko.notebook.WRITE_DATABASE"
            android:exported="true"
            />

    </application>

</manifest>